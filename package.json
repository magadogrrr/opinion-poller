    "cookie-session": "^1.4.0",
    "express": "^4.17.3",
    "mongoose": "^5.9.2",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "stripe": "^8.24.0"
  }
}
