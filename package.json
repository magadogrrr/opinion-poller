{
  "name": "server",
  "version": "1.0.0",
  "description": "Provides survey feedback services by email",
  "main": "index.js",
  "engines": {
    "node": "^11.10.1",
    "npm": "^6.7.0"
  },
  "scripts": {
    "start": "node index.js",
